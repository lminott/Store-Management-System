
public class Product {
   public String name;
   public int price;
   public int initialAmount;
   public int currentAmount;
   public Product(String name, int price,int initialAmount, int currentAmount ) {
	   this.name=name;
	   this.price=price;
	   this.initialAmount=initialAmount;
	   this.currentAmount=currentAmount;
   }
   
}
